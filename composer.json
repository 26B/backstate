{
    "name": "26b/backstate",
    "description": "Package with blade and livewire components",
    "license": "MIT",
    "authors": [
        {
            "name": "26B",
            "email": "hello@26b.io"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.org"
        },
        { "packagist": false }
    ],
    "require": {
        "php": "^8.1 || ^7.4"
    },
    "require-dev": {
        "laravel/framework": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "TwentySixB\\BackState\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TwentySixB\\BackState\\BackStateServiceProvider"
            ]
        }
    }
}
